{
  "name": "statickit-js",
  "version": 2,
  "alias": "js.statickit.com",
  "scope": "statickit",
  "builds": [
    {
      "src": "dist/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/statickit.js",
      "headers": { "cache-control": "public, max-age=300" },
      "dest": "/dist/statickit.min.js"
    }
  ]
}
